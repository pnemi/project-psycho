{
  "name": "project-psycho",
  "version": "1.0.0",
  "description": "Backend for Project Psycho game.",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "postinstall": "./node_modules/.bin/babel src -d dist",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon src/server.js --exec babel-node",
    "import:dev": "cross-env NODE_ENV=development babel-node src/fixtures/import.js",
    "import:prod": "cross-env NODE_ENV=production babel-node src/fixtures/import.js",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnemi/project-psycho.git"
  },
  "author": "Petr Němeček",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pnemi/project-psycho/issues"
  },
  "homepage": "https://github.com/pnemi/project-psycho#readme",
  "dependencies": {
    "apollo-server-express": "^2.8.1",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "mongoose": "^5.6.8",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.19.1",
    "standard": "^13.1.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "yarn": "1",
    "npm": "unsupported",
    "node": "10"
  },
  "eslintConfig": {
    "extends": "./config/.eslintrc.js"
  }
}
