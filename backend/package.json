    "url": "https://github.com/pnemi/project-psycho/issues"
  },
  "homepage": "https://github.com/pnemi/project-psycho#readme",
  "dependencies": {
    "apollo-server": "^2.0.7",
    "config": "^2.0.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "mongoose": "^5.2.13",
    "nodemon": "^1.18.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "eslint": "^5.5.0",
