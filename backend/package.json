{
  "name": "project-psycho",
  "version": "1.0.0",
  "description": "Backend for Project Psycho game.",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "postinstall": "./node_modules/.bin/babel src -d dist",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon src/server.js --exec babel-node",
    "import:dev": "cross-env NODE_ENV=development babel-node src/fixtures/import.js",
    "import:prod": "cross-env NODE_ENV=production babel-node src/fixtures/import.js",
    "lint": "eslint",
    "test": "standard && cross-env NODE_ENV=test mocha --require @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnemi/project-psycho.git"
  },
  "author": "Petr Němeček",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pnemi/project-psycho/issues"
  },
  "homepage": "https://github.com/pnemi/project-psycho#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "apollo-server-express": "^2.4.8",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "graphql": "^14.2.0",
    "mongoose": "^5.4.20",
    "nodemon": "^1.18.10"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.0.2",
    "standard": "^12.0.1"
  }
}
